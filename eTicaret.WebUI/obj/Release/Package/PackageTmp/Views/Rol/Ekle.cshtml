
@{
    ViewBag.Title = "Rol Ekle";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}

<br /><br />
<br /><br />
<div class="row">
    <div class="col-lg-12">
        <section class="panel">
            <header class="panel-heading">
                Rol Ekle
            </header>
            <div class="panel-body">
                <form action="/Rol/Ekle" class="form-horizontal tasi-form" enctype="multipart/form-data" method="post">
                    <div class="form-group">
                        <label class="col-sm-2 col-sm-2 control-label">Rol Adı:</label>
                        <div class="col-sm-10">
                            <input type="text" class="form-control" name="RolAdi">
                        </div>
                    </div>
                    <button class="btn btn-danger" type="submit">Kaydet</button>
                </form>
            </div>
        </section>
    </div>
</div>
